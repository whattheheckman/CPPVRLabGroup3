code = "shader_type spatial;
uniform sampler2D noise;
uniform float Displacement;
uniform float EmissionAmount;



void vertex() {
// Input:3
	vec3 n_out3p0 = VERTEX;


// Input:4
	vec3 n_out4p0 = NORMAL;


// FloatParameter:9
	float n_out9p0 = Displacement;


// VectorOp:10
	vec3 n_out10p0 = n_out4p0 * vec3(n_out9p0);


// VectorOp:6
	vec3 n_out6p0 =  * n_out10p0;


// VectorOp:5
	vec3 n_out5p0 = n_out3p0 + n_out6p0;


// Output:0
	VERTEX = n_out5p0;


}

void fragment() {
// Input:2
	vec4 n_out2p0 = COLOR;


// FloatParameter:3
	float n_out3p0 = EmissionAmount;


// VectorOp:4
	vec3 n_out4p0 = vec3(n_out2p0.xyz) * vec3(n_out3p0);


// Output:0
	ALBEDO = vec3(n_out2p0.xyz);
	EMISSION = n_out4p0;


}
